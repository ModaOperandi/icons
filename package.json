{
  "name": "moda-icons",
  "version": "0.0.1",
  "description": "Site-wide icon library for modaoperandi.com",
  "author": "dzucconi",
  "license": "MIT",
  "repository": "https://github.com/ModaOperandi/moda-icons",
  "main": "dist/index.js",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "NODE_ENV=production yarn optimize && node scripts/build",
    "release": "yarn build && cd dist && yarn publish",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "optimize": "rm -rf src/svg && svgo source/svg -o src/svg --config=svgoconfig.yml"
  },
  "peerDependencies": {
    "react": ">=16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.1.4",
    "del": "^5.0.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "svgo": "^1.3.0"
  }
}
